{
  "name": "@amcore/shared",
  "version": "0.0.1",
  "private": true,
  "description": "Shared schemas, types, and constants for AMCore",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "zod": "^3.24.0"
  },
  "devDependencies": {
    "@amcore/typescript-config": "workspace:*",
    "tsup": "^8.5.1",
    "typescript": "^5"
  }
}
